---
import MainHead from "./MainHead.astro"
import Footer from "../components/Footer.astro"
import Nav from "../components/Nav.astro"

interface Props {
  title: string;
  description: string;
}

const {
  title = "Default title",
  description = "Welcome to my deepness",
} = Astro.props as Props
---

<html lang="en">
<MainHead {title} {description} />
<body>
<Nav />
<main>
  <slot />
</main>
<Footer />
</body>
</html>
